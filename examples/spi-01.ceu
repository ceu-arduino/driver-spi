#include "gpio.ceu"
#include "wclock.ceu"
#include "spi.ceu"

output high/low PIN_09;

_Serial.begin(9600);
_Serial.println("=");

emit PIN_09(low);

spawn Spi();

loop do
    watching SPI_Transaction(1400000, SPI_MSBFIRST, SPI_MODE2, 9, _) do
        var byte v = await SPI_Transfer(_);
        _Serial.println("Data is:");_Serial.println(v,{BIN});
        {_DELAY(100);}
    end
end
